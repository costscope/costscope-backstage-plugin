{
  "name": "@costscope/backstage-plugin",
  "version": "0.1.1",
  "description": "Backstage frontend plugin for multi-cloud FinOps cost insights (FOCUS-native)",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "backstage",
    "backstage-plugin",
    "backstage-frontend-plugin",
    "finops",
    "cloud-cost",
    "cost-management",
    "cost-optimization",
    "developer-portal",
    "focus-spec",
    "focus",
    "multi-cloud",
    "aws",
    "azure",
    "gcp",
    "react",
    "material-ui",
    "recharts",
    "tanstack-query",
    "costscope"
  ],
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/costscope/costscope-backstage-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/costscope/costscope-backstage-plugin/issues"
  },
  "homepage": "https://costscope.github.io/costscope-backstage-plugin/",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "backstage": {
    "role": "frontend-plugin"
  },
  "dependencies": {
    "@costscope/contracts": "^0.1.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.17.0",
    "@mui/material": "^5.17.0",
    "@mui/utils": "^5.17.0",
    "@backstage/plugin-catalog-react": "^1.15.0",
    "@tanstack/react-query": "^5.56.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "recharts": "^2.7.2"
  },
  "devDependencies": {
    "axios": "^1.12.0",
    "@babel/core": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@backstage/cli": "^0.27.0",
    "@backstage/core-app-api": "^1.14.0",
    "@backstage/core-plugin-api": "^1.10.9",
    "@backstage/eslint-plugin": "^0.1.0",
    "@backstage/plugin-catalog": "^1.19.0",
    "@backstage/plugin-catalog-react": "^1.15.0",
    "@eslint/js": "^9.9.0",
    "@microsoft/api-extractor": "^7.47.0",
    "@playwright/test": "^1.48.0",
    "@size-limit/esbuild": "^11.2.0",
    "@size-limit/esbuild-why": "^11.2.0",
    "@size-limit/file": "^11.1.4",
    "@mui/icons-material": "^5.17.0",
    "@mui/material": "^5.17.0",
    "@mui/utils": "^5.17.0",
    "@storybook/react-webpack5": "^10.0.1",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/react-query": "^5.56.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@zodios/core": "^10.9.6",
    "babel-loader": "^10.0.0",
    "chromatic": "^11.0.0",
    "cors": "^2.8.5",
    "esbuild": "^0.21.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "express": "^4.19.2",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "size-limit": "^11.1.4",
    "storybook": "^10.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "5.3.3",
    "os-browserify": "^0.3.0",
    "tty-browserify": "^0.0.1",
    "eslint-plugin-storybook": "10.0.1",
    "@storybook/addon-docs": "^10.0.1"
  },
  "scripts": {
    "verify": "yarn lint && yarn typecheck && yarn test && yarn size",
    "verify:deps": "echo 'No dependency verification step defined; skipping'",
    "api:diff": "yarn types:emit && node ../../scripts/api-report-diff.js",
    "spec:diff": "node ../../scripts/openapi-diff.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests --coverage",
    "test:dev": "NODE_OPTIONS=--experimental-vm-modules SKIP_COVERAGE=1 jest --passWithNoTests --coverage=false",
    "test:fast": "node scripts/test-fast-runner.cjs",
    "test:widgets": "NODE_OPTIONS=--experimental-vm-modules SKIP_COVERAGE=1 jest --passWithNoTests --coverage=false --testPathPattern widgets",
    "test:widgets:fast": "FAST_TESTS=1 NODE_OPTIONS=--experimental-vm-modules SKIP_COVERAGE=1 jest --passWithNoTests --coverage=false --testPathPattern widgets --maxWorkers=50%",
    "test:e2e": "playwright install --with-deps chromium && playwright test -c ../../playwright.config.ts",
    "test:e2e:headed": "playwright test -c ../../playwright.config.ts --headed",
    "test:e2e:ui": "playwright test -c ../../playwright.config.ts --ui",
    "test:e2e:quick": "E2E_USE_MINIMAL=1 E2E_REUSE=1 PLAYWRIGHT_BROWSERS=chromium playwright test -c ../../playwright.config.ts --grep @smoke",
    "test:e2e:smoke:fresh": "E2E_REUSE=0 PLAYWRIGHT_BROWSERS=chromium playwright test -c ../../playwright.config.ts --grep @smoke",
    "test:e2e:reuse": "E2E_REUSE=1 PLAYWRIGHT_BROWSERS=chromium playwright test -c ../../playwright.config.ts",
    "e2e:fast": "yarn test:e2e:quick",
    "e2e:full": "yarn test:e2e",
    "e2e:servers": "run-s link:example build:size && run-p mock example:start",
    "e2e:dev": "run-s link:example build:size && run-p mock example:start && PLAYWRIGHT_BROWSERS=chromium E2E_REUSE=1 playwright test --headed --grep @smoke",
    "e2e:smoke": "PLAYWRIGHT_BROWSERS=chromium E2E_REUSE=1 playwright test --grep @smoke",
    "dev:loop": "run-s link:example build:size && run-p build:watch example:start",
    "backend:start": "yarn workspace mock-backend start",
    "backend:start:demo": "FRONTEND_ORIGIN=\"http://localhost:${APP_PORT:-3000}\" PORT=${PORT:-7008} yarn workspace mock-backend start",
    "link:example": "node ../../scripts/link-example-plugin.js",
    "dev:full": "run-s link:example build:size && FRONTEND_ORIGIN=\"http://localhost:${APP_PORT:-3000}\" PORT=${PORT:-7008} run-p mock:start:demo backend:start build:watch example:start",
    "demo": "node scripts/demo.mjs",
    "mock:start:demo": "PORT=${MOCK_PORT:-7007} node ../../scripts/mock-server.cjs",
    "demo:inner": "run-s link:example build:size && run-p mock:start:demo backend:start:demo build:watch example:start",
    "demo:front": "run-s link:example build:size && run-p build:watch example:start",
    "demo:front:inner": "run-s link:example build:size && run-p build:watch example:start",
    "clean:dist": "rimraf dist temp tsconfig.tsbuildinfo tsconfig.types.tsbuildinfo",
    "clean:caches": "rimraf .jest-cache ../../examples/*/packages/app/node_modules/.cache || true",
    "clean:stale": "run-s clean:dist clean:caches",
    "dev:full:reset": "run-s clean:stale dev:full",
    "example:dev": "yarn build:size && node ../../scripts/dev-example.cjs",
    "e2e:headful": "PW_HEADFUL=1 playwright test",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "playwright test -c ../../tests-storybook/playwright.storybook.config.ts",
    "test:storybook:quick": "SB_MAX_STORIES=50 playwright test -c ../../tests-storybook/playwright.storybook.config.ts",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --only-changed",
    "typecheck": "tsc -p tsconfig.json --pretty false",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 tsup --config tsup.config.ts && yarn types:emit",
    "build:no-map": "NODE_OPTIONS=--max-old-space-size=4096 tsup --config tsup.config.ts && yarn types:emit",
    "build:size": "tsup --config tsup.config.ts",
    "build:watch": "tsup --config tsup.config.ts --watch",
    "build:watch:fast": "tsup --config tsup.config.ts --watch --sourcemap=false --splitting false",
    "typecheck:watch": "tsc -p tsconfig.json --watch --preserveWatchOutput false",
    "example:start": "yarn workspace app start",
    "dev": "run-p mock build:watch",
    "size": "NODE_ENV=production yarn build:size && size-limit --config ../../.size-limit.cjs",
    "clean": "rimraf dist dist-types temp coverage .jest-cache tsconfig.tsbuildinfo tsconfig.types.tsbuildinfo",
    "spec:hash": "yarn workspace @costscope/contracts spec:hash",
    "spec:descriptor": "node ../../scripts/spec-descriptor-check.js",
    "spec:check": "yarn spec:hash && yarn spec:descriptor",
    "diagnose": "node ../../scripts/diagnose.js",
    "pack:local": "yarn build:size && npm pack --pack-destination temp",
    "audit": "npm audit --omit=dev",
    "audit:sarif": "osv-scanner --lockfile=../../yarn.lock --format sarif > audit.osv.sarif || echo 'Install osv-scanner (https://github.com/google/osv-scanner) to generate SARIF'",
    "resolutions:check": "node ../../scripts/check-resolutions.js",
    "release:check": "node ../../scripts/release-check.js",
    "preversion": "echo 'Use changesets via PR workflow instead of npm version' && exit 1",
    "version": "echo 'Versioning handled by changesets/action'",
    "postversion": "echo 'Postversion noop (handled in CI)'",
    "mock": "node ../../scripts/mock-server.cjs",
    "smoke": "node ../../scripts/smoke.js",
    "smoke:ssr": "node ../../scripts/smoke-ssr.js",
    "exp:sdk-size": "node ../../scripts/experiment-sdk-size.cjs",
    "analyze": "node ../../scripts/analyze.js",
    "types:emit": "node --max-old-space-size=4096 ../../node_modules/.bin/tsc -p ../../tsconfig.types.json",
    "api:snapshot": "yarn build:size && yarn types:emit && node ../../scripts/check-dist-types.js && api-extractor run --local --verbose",
    "api:check": "yarn build:size && yarn types:emit && node ../../scripts/check-dist-types.js && api-extractor run --verbose",
    "api:check:strict": "yarn build:size && yarn types:emit && node ../../scripts/api-check-strict.js",
    "dist-smoke": "node ./src/__tests__/integration/runner-bundle-client.cjs"
  },
  "_notes": {
    "size-limit": "Budgets enforced via repo-level ../../.size-limit.cjs; local section removed to avoid drift."
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "resolutions": {
    "@backstage/core-plugin-api": "1.10.9",
    "@backstage/errors": "1.2.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "axios": "^1.7.4"
  },
  "overrides": {
    "axios": "^1.12.0"
  }
}
